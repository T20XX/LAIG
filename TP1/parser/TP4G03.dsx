        </component>
		
		<component id="rectLeft">
         
            <transformation>
				<translate x="0" y="0" z="0.5" />
			</transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" />
			
            <children>
                <primitiveref id="unit_rectangle" />
            </children>
            
        </component>
		        
		<component id="rectFront">
         
            <transformation>
				<rotate axis=y angle= 45 />
				<translate x="0.5" y="0" z="0" />
			</transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" />
			
            <children>
                <primitiveref id="unit_rectangle" />
            </children>
            
        </component>
		
		<component id="rectBack">
         
            <transformation>
				<rotate axis=y angle= -45 />
				<translate x="-0.5" y="0" z="0" />
			</transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" />
			
            <children>
                <primitiveref id="unit_rectangle" />
            </children>
            
        </component>
    </components>
	
	
  

</dsx>